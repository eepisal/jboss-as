<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" metadata-complete="false" version="3.0" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd">
    <enterprise-beans>
        <session>
            <ejb-name>AssemblyBean</ejb-name>
            <!--business-local>com.sun.ts.tests.ejb30.assembly.common.AssemblyLocalIF</business-local-->
            <business-remote>org.jboss.as.test.integration.ejb.interceptor.assembly.AssemblyRemoteIF</business-remote>
            <ejb-class>org.jboss.as.test.integration.ejb.interceptor.assembly.AssemblyBean</ejb-class>
            <session-type>Stateless</session-type>
            <transaction-type>Container</transaction-type>
            <security-identity>
                <use-caller-identity/>
            </security-identity>
        </session>
    </enterprise-beans>
    <interceptors>
        <interceptor>
            <interceptor-class>org.jboss.as.test.integration.ejb.interceptor.assembly.AssemblyInterceptor</interceptor-class>
        </interceptor>
    </interceptors>

    <assembly-descriptor>
        <interceptor-binding>
            <ejb-name>AssemblyBean</ejb-name>
            <interceptor-order>
                <interceptor-class>org.jboss.as.test.integration.ejb.interceptor.assembly.AssemblyInterceptor</interceptor-class>
            </interceptor-order>
        </interceptor-binding>
    </assembly-descriptor>

</ejb-jar>
